{
  "name": "campus-map-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "export:sqlite:csv": "ts-node scripts/sqlite-export-csv.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "seed": "ts-node src/seeds/seed.ts",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:run": "ts-node ./node_modules/typeorm/cli.js migration:run --dataSource ./data-source.ts",
    "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource ./data-source.ts",
    "typeorm:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --dataSource ./data-source.ts",
    "regenerate:offline-pack": "ts-node -r tsconfig-paths/register ./scripts/regenerate-offline-pack.ts",
    "psql:migrate": "psql \"$DATABASE_URL\" -f migrations/0001_init.sql"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "better-sqlite3": "^12.4.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.0",
    "node-fetch": "^2.6.7",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
